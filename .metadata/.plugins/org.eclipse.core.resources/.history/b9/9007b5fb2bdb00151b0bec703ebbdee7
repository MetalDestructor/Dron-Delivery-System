package managers;

import java.util.ArrayList;
import java.util.List;

import domain.Drone;

public class DroneManager {

	List<Drone> drones;
	
	DroneManager(){
		drones=new ArrayList<>();
		for(int i=0; i<50;i++){
			drones.add(new Drone(2000,5,500));
		}
		for(int i=0; i<30;i++){
			drones.add(new Drone(1200,3,200));
		}
	}
}
