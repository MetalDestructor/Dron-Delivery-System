package request;

import java.util.Date;

public abstract class Request {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

	private String id;
	private Date date;
	
	public Request(String id){
		setId(id);
		this.date=new Date();
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public Date getDate() {
		return date;
	}

	
	
}